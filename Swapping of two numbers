#include<stdio.h>

int main()

{

int var1, var2, temp;

printf("Enter two integers");

scanf("%d%d", &var1, &var2);

printf("Before swapping_First variable=%d_second variable=%d", var1,var2);
temp=var1;
var1=var2;
var2=temp;
printf("After swapping_First variable=%d_second variable=%d",var1, var2);
return 0;
}
